{
  "github.copilot.nextEditSuggestions.enabled": true,
  "editor.fontFamily": "'Monaspace Radon', 'Cascadia Code', Menlo, Monaco, 'Courier New', monospace",
  "editor.inlineSuggest.fontFamily": "'Monaspace Radon', 'Cascadia Code', Menlo, Monaco, 'Courier New', monospace",
  "editor.inlayHints.enabled": "on",
  "editor.inlayHints.fontFamily": "'Monaspace Radon', 'Cascadia Code', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": "'calt', 'ss01'",
  "editor.fontSize": 16,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  },
  "[blade]": {
    "editor.defaultFormatter": "shufo.vscode-blade-formatter"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "git.autofetch": true,
  "explorer.compactFolders": false,
  "editor.guides.bracketPairs": "active",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "workbench.tree.indent": 15,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "trailing",
  "editor.linkedEditing": true,
  "editor.suggest.insertMode": "replace",
  "files.autoSave": "onFocusChange",
  "php.executablePath": "/usr/local/bin/php",
  "php.validate.executablePath": "/usr/local/bin/php",
